{
    "name": "jmichaelward/wp-plugin-starter",
    "description": "A bootstrap library to help you quickly scaffold WordPress plugins.",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Jeremy Ward",
            "email": "jeremy@jmichaelward.com",
            "homepage": "https://jmichaelward.com",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "keywords": [
        "wordpress",
        "plugin",
        "scaffold"
    ],
    "require": {},
    "require-dev": {
        "jmichaelward/wp-plugin-starter-init": "dev-master@dev"
    },
    "scripts": {
        "plugin-init": "php ./vendor/bin/wp-plugin-starter"
    },
    "autoload": {
        "psr-4": {
            "JMichaelWard\\WPPluginStarter\\": "src/"
        }
    },
    "suggest": {
        "webdevstudios/oops-wp": "Provides interfaces and abstract classes to support object-oriented WordPress development."
    }
}
